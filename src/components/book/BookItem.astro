---
export interface Props {
    id: string;
    title: string;
    img: string;
    three?: string;
}

const { id, title, img, three } = Astro.props;
---

<div class="border-2 rounded-2xl aspect-square">
    <a href=`/${id}`>
        <img
            class="aspect-[500/500] object-contain"
            src=`${img}`
            alt=`${title}`
        >
    </a>
</div>