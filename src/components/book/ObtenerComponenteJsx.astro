---
// ESTE COMPONENTE ES PARA CARGAR LOS COMPONENTES DE TRES DIMENSIONES PUESTO 
// QUE NO SE RECORRER LOS COMPONENTE DE UNA CARPETA

import B1Cerrada from "../three/B1Cerrada";
import BandejaAcotada from "../three/BandejaAcotada";
import EstucheConAsas from "../three/EstucheConAsas";

const { componente_jsx } = Astro.props

---

    {
        componente_jsx === "EstucheConAsas" ? <EstucheConAsas client:visible /> : null
    }
    {
        componente_jsx === "BandejaAcotada" ? <BandejaAcotada client:visible /> : null
    }
    {
        componente_jsx === "B1Cerrada" ? <B1Cerrada client:visible /> : null
    }